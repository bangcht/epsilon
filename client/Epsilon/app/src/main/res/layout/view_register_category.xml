                genius:g_scrubberColor="#00b153"
                genius:g_thumbColor="#217f4d"
                genius:g_trackColor="#ff925c"
                genius:g_tickSize="8sp"
                genius:g_touchSize="12sp"
                genius:g_thumbSize="10sp"
                genius:g_min="1"
                genius:g_max="5"
