<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:genius="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_background"
    android:orientation="vertical"
    android:paddingLeft="24dp"
    android:paddingRight="24dp"
    android:paddingTop="50dp">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="32dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/category1"
            android:textColor="@android:color/white"/>

        <net.qiujuer.genius.widget.GeniusSeekBar
            android:id="@+id/seekBar3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="5dp"
            genius:g_allowTrackClickToDrag="false"
            genius:g_indicatorBackgroundColor="#0cb45a"
            genius:g_indicatorFormatter="%d"
            genius:g_mirrorForRtl="true"
            genius:g_indicatorTextAppearance="@style/CustomFloaterTextAppearance"
            genius:g_rippleColor="#0cb45a"
            genius:g_scrubberColor="#00b153"
            genius:g_thumbColor="#217f4d"
            genius:g_trackColor="#ff925c"
            genius:g_tickSize="8sp"
            genius:g_touchSize="12sp"
            genius:g_thumbSize="10sp"
            genius:g_min="1"
            genius:g_max="9"
            genius:g_scrubberStroke="4dp"
            genius:g_trackStroke="4dp"
            genius:g_value="3"
            android:layout_marginTop="16dp"/>

    </LinearLayout>



</LinearLayout>